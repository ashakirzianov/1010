{
    "extends": "tslint:latest",
    "rules": {
        "interface-over-type-literal": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "no-duplicate-imports": false,
        "no-implicit-dependencies": [true, "dev"],
        "max-line-length": [
            true,
            150
        ],
        "member-access": [
            false
        ],
        "arrow-parens": false,
        "ordered-imports": [
            false
        ],
        "new-parens": false,
        "interface-name": [
            false
        ],
        "no-namespace": false,
        "prefer-for-of": false,
        "max-classes-per-file": [
            false
        ]
    }
}